<script type="text/javascript">
$('.mapbox-link').click(function() {
	var $this = $(this);
	var name = $this.attr("name");
	$data = CompanyAddress[name];
	console.log("$data=");
	console.log($data);
    //$data=$(this).data('json');
    $('.mapbox-link').removeClass('active');
    $(this).addClass('active');
    $('#mapbox').attr('src', $data.url);
    $(this).parent().parent().find('.title').text($data.title);
    $(this).parent().parent().find('.address').text($data.address);
    $('#contacts').html('');
    $('#contacts').append($.map($data.items, function(itm) {
        return tmpl('tmpl-card', itm);
    }));
});
$('.mapbox-link:first-child').trigger('click');
</script>
