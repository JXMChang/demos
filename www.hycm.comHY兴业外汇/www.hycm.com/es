<section id="block-block-12" class="block block-block clearfix">

      
   <h2 class="text-center" premios="" de="" mercados="" financieros="" internacionales="">

<div class="padd-top">       
     <div class="col-md-3 col-sm-6 col-xs-12 ">
       <div class=" com-hycm-spr knowledge trophy display-table-cell"></div>
        <div class="display-table-cell">
          <h4 class="latoM textR">FXDailyInfo</h4>
          <h4 class="latoL">Mejor Broker Forex<br />Europa 2017</h4>
       </div>
</div>

     <div class="col-md-3 col-sm-6 col-xs-12 ">
