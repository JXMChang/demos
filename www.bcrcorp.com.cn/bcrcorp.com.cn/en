//loadMarketAnalysis();

function normalizeNumber(num) {
	var t = parseInt(num)
	if(isNaN(t) ) {
		return 0;
	}

	return t/100;
}

function draw_chart(id1,id2,long,short){
	var bar1 = new ProgressBar.SemiCircle(id1, {
	  strokeWidth: 6,
	  easing: 'easeInOut',
	  duration: 1400,
	  color: '#0da4e5',
	  trailColor: '#ccc',
	  trailWidth: 1,
	  svgStyle: null
	});

	var bar2 = new ProgressBar.SemiCircle(id2, {
	  strokeWidth: 6,
	  easing: 'easeInOut',
	  duration: 1400,
	  color: '#d33939',
	  trailColor: '#ccc',
	  trailWidth: 1,
	  svgStyle: null
	});

	bar1.animate(long);
	bar2.animate(short);
}

jQuery(document).ready(function(){

	
	jQuery('.counter').counterUp({
		delay: 10,
		time: 1000
	});

	jQuery('.content-slider').owlCarousel({
		baseClass: "cs-",
		nav: true,
		navText: ["<span class=\"fa fa-chevron-left\"></span>", "<span class=\"fa fa-chevron-right\"></span>"],
		responsiveClass: true,
		responsive:{
			0:{
				items:1,
				nav:false
			},
			480:{
				items:1,
				nav:false
			},
			767:{
				items:1,
				nav:false
			},
			768:{
				items:2,
				nav:false
			},
			992:{
				items:3,
				nav:false
			},
			1024:{
				items:3,
				nav:false
			},
			1200:{
				items:5,
				nav:true,
				loop:false
			}
		},
	});
});
