        function displaySubMenu(li) {
            var subMenu = li.getElementsByTagName("ul")[0];
            if(subMenu!=undefined) subMenu.style.display = "block";
        }
        function hideSubMenu(li) {
            var subMenu = li.getElementsByTagName("ul")[0];
            if (subMenu != undefined) subMenu.style.display = "none";
        }
    </script>

