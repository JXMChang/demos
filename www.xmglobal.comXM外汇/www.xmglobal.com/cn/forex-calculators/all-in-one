</script><script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-41817274-1', 'auto');
    ga('send', 'pageview');

</script><script>
    function setGoogleCookie(name, value, days) {
        var date = new Date();
        date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
        var expires = "; expires=" + date.toGMTString(),
            domainRegex = new RegExp(/[^.]+\.([a-zA-Z]{3,}|[a-zA-Z]{2}\.[a-zA-Z]{2})$/),
            host = window.location.host,
            domain = "; domain=." + host.match(domainRegex)[0];
        document.cookie = name + "=" + value + expires + domain + ";path=/";
    }
    function getParam(p) {
        var match = new RegExp('[?&]' + p + '=([^&]*)').exec(window.location.search);
        return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
    }
    function readCookie(name) {
        var n = name + "=";
        var cookie = document.cookie.split(';');
        for (var i = 0; i < cookie.length; i++) {
            var c = cookie[i];
            while (c.charAt(0) == ' ')
                c = c.substring(1, c.length);
            if (c.indexOf(n) == 0)
                return c.substring(n.length, c.length);
        }
        return '';
    }

    ga(function (tracker) {
        var gaClientId = tracker.get('clientId'),
            gclid = getParam('gclid');
        if (gclid) {
            var gclsrc = getParam('gclsrc');
            if (!gclsrc || gclsrc.indexOf('aw') !== -1) {
                setGoogleCookie('gclid', gclid, 90);
            }
        } else {
            gclid = readCookie('gclid');
        }

        if (document.getElementsByName('ga_client_id') && document.getElementsByName('gclid')) {
            document.getElementsByName('ga_client_id')[0].setAttribute('value', gaClientId);
            document.getElementsByName('gclid')[0].setAttribute('value', gclid);
        }
    });
</script><script>
$(document).ready(function() {

    $.validator.setDefaults({ ignore: '' });

    
    $('form.all-in-one').each(function () {
        
        $(this).validate({

            
            rules: {
                account_currency: { required: true,  }, currency_pair: { required: true,  }, account_type: { required: true,  }, lots: { required: true, number: true,  }, leverage: { required: true,  },             },

            messages: {
                account_currency: { required: "<strong>基础账户货币：</strong> 部分是必须填写的",  }, currency_pair: { required: "<strong>货币对</strong> 部分是必须填写的",  }, account_type: { required: "<strong>账户类型</strong> 部分是必须填写的",  }, lots: { required: "<strong>交易手数</strong> 部分是必须填写的", number: " <strong>交易手数</strong> 仅限填写数字",  }, leverage: { required: "<strong>交易杠杆</strong> 部分是必须填写的",  },             },

            ignore: '.ignore_validation',

            highlight: function (element, errorClass, validClass) {
                $(element)
                    .addClass(errorClass)
                    .removeClass(validClass);

                if ($(element).attr('type') != 'checkbox' && $(element).attr('type') != 'radio') {
                    var selectClass = ($(element).prop('type') == 'select-one') ? ' select' : '',
                        icon = '<div class="glyphicon-wrapper"><span class="glyphicon glyphicon-remove form-control-feedback'+ selectClass +'" aria-hidden="true"></span></div>';
                    $(element)
                        .closest('.form-group')
                        .removeClass('has-success')
                        .addClass('has-error')
                        .end()
                        .siblings('.glyphicon-wrapper').remove()
                        .end()
                        .before(icon);
                }

            },
            unhighlight: function (element, errorClass, validClass) {
                $(element)
                    .removeClass(errorClass)
                    .addClass(validClass);

                if ($(element).attr('type') != 'checkbox' && $(element).attr('type') != 'radio') {
                    var selectClass = ($(element).prop('type') == 'select-one') ? ' select' : '',
                        icon = '<div class="glyphicon-wrapper"><span aria-hidden="true" class="glyphicon glyphicon-ok form-control-feedback'+ selectClass +'"></span></div>';
                    $(element)
                        .closest('.form-group')
                        .removeClass('has-error')
                        .addClass('has-success')
                        .end()
                        .siblings('.glyphicon-wrapper').remove()
                        .end()
                        .before(icon);

                    if ($(element).parents('.remove-valid').length) {

                        $(element)
                            .removeClass('valid')
                            .closest('.form-group')
                            .removeClass('has-success')
                            .end()
                            .siblings('.glyphicon-wrapper').remove();
                    }
                }
            },

            errorPlacement: function (error, element) {
                
                if (element.siblings('.input-group-addon').length > 0) {
                    $(element)
                        .closest('.input-group')
                        .after(error);
                }

                else if (element.closest('.form-group').find('.search-select').length > 0) {
                    $(element)
                        .closest('.form-group').find('.select2-container')
                        .after(error);
                }

                else if (element.closest('.form-group').find('.btn-group').length > 0) {
                    $(element)
                        .closest('.form-group').find('.btn-group')
                        .after(error);
                }

                else if (element.attr('type') == 'file') {
                    $(element)
                        .closest('.input-group')
                        .after(error);
                }
                else if (element.attr('type') == 'checkbox') {
                    $(element)
                        .closest('.form-group')
                        .append(error);
                }
                else if (element.attr('type') == 'radio') {
                    $(element)
                        .closest('label')
                        .siblings('span')
                        .after(error);
                }
                else {
                    error.insertAfter(element);
                }
            },

            invalidHandler: function (form, validator) {
                scrollToObject($(validator.errorList[0].element));
            },

            submitHandler: function (form) {
                                    $(form).find('button').text(pleaseWait);
                    form.submit();
                    return false;
                            }

        });

    });
    
    $('form#pre-chat-form,#pre-chat-form-member').each(function () {
        
        $(this).validate({

            
            rules: {
                first_name: { required: true,  }, last_name: { required: true,  }, email: { required: true, email: true,  }, user_id: { required: true, regex: /^[0-9]+$/,  }, user_password: { required: true, regex: /^[a-zA-Z0-9\#\[\]\(\)\@$\&\*\!\?\|\%\,\.\^\/\\+_-]+$/,  }, preferred_language: { required: true,  },             },

            messages: {
                first_name: { required: "<strong>名</strong> 部分是必须填写的",  }, last_name: { required: "<strong>姓</strong> 部分是必须填写的",  }, email: { required: "<strong>邮件 EMAIL</strong> 部分是必须填写的", email: "<strong>邮件 EMAIL</strong> 必须包含有效的邮箱地址",  }, user_id: { required: "<strong>账户号</strong> 部分是必须填写的", regex: "该 账户号 字段无效",  }, user_password: { required: "<strong>密码：</strong> 部分是必须填写的", regex: "<strong>密码：</strong> 必须包含有效的邮箱地址",  }, preferred_language: { required: "<strong>首选语言</strong> 部分是必须填写的",  },             },

            ignore: '.ignore_validation',

            highlight: function (element, errorClass, validClass) {
                $(element)
                    .addClass(errorClass)
                    .removeClass(validClass);

                if ($(element).attr('type') != 'checkbox' && $(element).attr('type') != 'radio') {
                    var selectClass = ($(element).prop('type') == 'select-one') ? ' select' : '',
                        icon = '<div class="glyphicon-wrapper"><span class="glyphicon glyphicon-remove form-control-feedback'+ selectClass +'" aria-hidden="true"></span></div>';
                    $(element)
                        .closest('.form-group')
                        .removeClass('has-success')
                        .addClass('has-error')
                        .end()
                        .siblings('.glyphicon-wrapper').remove()
                        .end()
                        .before(icon);
                }

            },
            unhighlight: function (element, errorClass, validClass) {
                $(element)
                    .removeClass(errorClass)
                    .addClass(validClass);

                if ($(element).attr('type') != 'checkbox' && $(element).attr('type') != 'radio') {
                    var selectClass = ($(element).prop('type') == 'select-one') ? ' select' : '',
                        icon = '<div class="glyphicon-wrapper"><span aria-hidden="true" class="glyphicon glyphicon-ok form-control-feedback'+ selectClass +'"></span></div>';
                    $(element)
                        .closest('.form-group')
                        .removeClass('has-error')
                        .addClass('has-success')
                        .end()
                        .siblings('.glyphicon-wrapper').remove()
                        .end()
                        .before(icon);

                    if ($(element).parents('.remove-valid').length) {

                        $(element)
                            .removeClass('valid')
                            .closest('.form-group')
                            .removeClass('has-success')
                            .end()
                            .siblings('.glyphicon-wrapper').remove();
                    }
                }
            },

            errorPlacement: function (error, element) {
                
                if (element.siblings('.input-group-addon').length > 0) {
                    $(element)
                        .closest('.input-group')
                        .after(error);
                }

                else if (element.closest('.form-group').find('.search-select').length > 0) {
                    $(element)
                        .closest('.form-group').find('.select2-container')
                        .after(error);
                }

                else if (element.closest('.form-group').find('.btn-group').length > 0) {
                    $(element)
                        .closest('.form-group').find('.btn-group')
                        .after(error);
                }

                else if (element.attr('type') == 'file') {
                    $(element)
                        .closest('.input-group')
                        .after(error);
                }
                else if (element.attr('type') == 'checkbox') {
                    $(element)
                        .closest('.form-group')
                        .append(error);
                }
                else if (element.attr('type') == 'radio') {
                    $(element)
                        .closest('label')
                        .siblings('span')
                        .after(error);
                }
                else {
                    error.insertAfter(element);
                }
            },

            invalidHandler: function (form, validator) {
                scrollToObject($(validator.errorList[0].element));
            },

            submitHandler: function (form) {
                                    $(form).find('button').text(pleaseWait);
                    form.submit();
                    return false;
                            }

        });

    });
    });
</script>
<script defer="defer" src="https://cloud.xm-cdn.com/assets/js/libraries/select2.min.js?ver=c54ebcf2f584daac853915c3964bd974"></script><script type="text/javascript">
$(document).ready(function() {
    $(".dp_currency_pair").select2({
				theme: "classic",
							});
});
</script>

