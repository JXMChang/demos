    <script type="text/javascript">
        function displaySubMenu(li) {
            var subMenu = li.getElementsByTagName("ul")[0];
            if(subMenu!=undefined) subMenu.style.display = "block";
        }
        function hideSubMenu(li) {
            var subMenu = li.getElementsByTagName("ul")[0];
            if (subMenu != undefined) subMenu.style.display = "none";
        }
    </script>

    <!-- 右侧浮动都电话图标，暂时隐藏掉
    <div id="clbh_phone_div" class="cbh-phone cbh-green cbh-show" style="bottom:100px;right:80px;">
        <div class="cbh-ph-circle"></div>
        <div class="cbh-ph-circle-fill"></div>
        <div class="cbh-ph-img-circle"></div>
    </div>
    -->
    
    
<script>
    /*无缝滚动*/
    window.onload = function () {
        var oDiv = document.getElementById('act_content101');
        var aUl = document.getElementById('carousel');
        var aLi = aUl.getElementsByTagName('li');
        var timer = null;
        function move() {
            if (aUl.offsetLeft < -aUl.offsetWidth / 2) {
                aUl.style.left = '0';
            }
            if (aUl.offsetLeft > 0) {
                aUl.style.left = -aUl.offsetWidth / 2 + 'px';
            }
            aUl.style.left = aUl.offsetLeft - 1 + 'px';
        };
        aUl.innerHTML = aUl.innerHTML + aUl.innerHTML;  //将4个li变成8个li
        aUl.style.width = aLi[0].offsetWidth * aLi.length + 'px';

        timer = setInterval(move, 30);

        oDiv.onmouseover = function () {
            clearInterval(timer);
        };
        oDiv.onmouseout = function () {
            timer = setInterval(move, 30);
        };
    };
</script>

    <!--网站引用的外部脚本都写在隐藏域中，不会影响页面效果-->
