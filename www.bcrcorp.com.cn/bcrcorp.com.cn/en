<script>

jQuery(document).ready(function(){
	//setTimeout(loadMarketAnalysis, 1000);

	jQuery.ajax({
		type: "GET",
		url: "/test",
		dataType: "html",
		error:function(x){},
		success:function(res){
			jQuery('div.pricefeed').html(res);
			//console.log(res);
			set_chart();
		}
	});
});


function set_chart()
{
	jQuery('.pf-item').each(function(){
		var longId = jQuery(this).find('.circular-progress.long').attr('id');
		var shortId = jQuery(this).find('.circular-progress.short').attr('id');
		var longPercentage = normalizeNumber(jQuery(this).find('.longPosition').text());
		var shortPercentage = normalizeNumber(jQuery(this).find('.shortPosition').text());
		draw_chart('#'+longId,'#'+shortId,longPercentage,shortPercentage);
	});
}


function loadMarketAnalysis()
{
	var item = jQuery('<a href="#" class="cs-item"></a>');
	/*
	<img src="/images/education/market_commentary_image_library/market_analysis_image1.jpg" />
			<div class="cs-info">
				<div class="cs-header">
					<span class="cs-date">MM/DD/YYYY (DAY)</span>
					<span class="cs-type">Market Commentary</span>
				</div>
				<h3 class="cs-title">Bacera Commentaries</h3>
				<p class="cs-desc">Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ulabore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco.</p>
			</div>
	 */
	var commentaris =''
