<script src="/Content/Scripts/js/numcalc.js"></script>
<script>


    var buyid = 1, sellid = 1;
    $("#btn_Search").click(function () {
        event.preventDefault();

        var mt4 = $("#mt4").val();
        var email = $("#email").val();
        var currency = $("#currency").val();
        if (mt4 == "") { alert("请填写交易账号"); $("#mt4").focus(); return false; }
        if (email == "") { alert("请填写邮箱"); $("#email").focus(); return false; }
        if (currency == "") { alert("请填写货币对"); $("#currency").focus(); return false; }

        $.post("/MarketDepth/SearchCurrency", { mt4: mt4, email: email, currency: currency }, function (data) {
            if (data.Result == 1) {
                //移除生成的li
                $("li").remove("[name=appendli]");
                $("#Symbol").html(currency);
                $("#incomingPrice").css("display", "inline");
                var inc = document.getElementById('incomming');

                var wsImpl = window.WebSocket || window.MozWebSocket;
                if (typeof (ws) != "undefined" && ws != null) {
                    ws.close();
                }

                window.ws = new wsImpl('ws://' + data.IP + '/');
                ws.onmessage = function (evt) {
                    var parsedJson = $.parseJSON(evt.data);
                    //MdEntryType 1买 0卖
                    if (parsedJson.MdEntryType == "1") {
                        var html = "<li  class=\"mq_top mq_top2\" id=buy" + buyid + " name=\"appendli\"><span class=\"mqs_l\">" + numAdd(parsedJson.MDEntryPx, data.BuySpread) + "</span><span class=\"mqs_r\">" + numDiv(parsedJson.MDEntrySize, data.ContractSize) + "</span></li>";

                        $("#ulbuy").after(html);
                        //1涨 -1跌
                        if (parsedJson.Change == 1) {
                            $("#buyicon").removeClass("fa-arrow-down").addClass("fa-arrow-up");
                        }
                        if (parsedJson.Change == -1) {
                            $("#buyicon").removeClass("fa-arrow-up").addClass("fa-arrow-down");
                        }

                        if (buyid > 5) {
                            $("#buy" + (buyid - 5)).remove();
                        }
                        if (buyid > 1) {
                            $("#buy" + (buyid - 1)).removeClass("mq_top mq_top2");
                        }
                        buyid += 1;

                    } else {
                        var html = "<li class=\"mq_top mq_top2\" id=sell" + sellid + " name=\"appendli\"><span class=\"mqs_l\">" + numDiv(parsedJson.MDEntrySize, data.ContractSize) + "</span><span class=\"mqs_r\">" + numSub(parsedJson.MDEntryPx, data.SellSpread) + "</span></li>";
                        $("#ulsell").after(html);
                        //1涨 -1跌
                        if (parsedJson.Change == 1) {
                            $("#sellicon").removeClass("fa-arrow-down").addClass("fa-arrow-up");
                        }
                        if (parsedJson.Change == -1) {
                            $("#sellicon").removeClass("fa-arrow-up").addClass("fa-arrow-down");
                        }

                        if (sellid > 5) {
                            $("#sell" + (sellid - 5)).remove();
                        }
                        if (sellid > 1) {
                            $("#sell" + (sellid - 1)).removeClass("mq_top mq_top2");
                        }
                        sellid += 1;
                    }

                };
                ws.onopen = function () {
                    ws.send(data.CurrencyName);
                };
                ws.onclose = function () {
                }



            } else {
                alert(data.Message);
            }
        })

    })
</script>
