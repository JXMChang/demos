<script type="text/javascript">
jQuery('#singleProductWindow').on('show.bs.modal', function (event) {
	var button = jQuery(event.relatedTarget) // Button that triggered the modal
	var symbol = button.data('symbol') // Extract info from data-* attributes

	var modal = jQuery(this)
	modal.find('.modal-title').text('产品细节');


	jQuery.get( "/pr/"+symbol+"/sim", function( data ) {
  		modal.find('.modal-body').html(data);
	});
	// If necessary, you could initiate an AJAX request here (and then do the updating in a callback).
	// Update the modal's content. We'll use jQuery here, but you could use a data binding library or other methods instead.
});

jQuery(function ($) { 
    $('#viewMoreProducts').click(function(){
		var text = document.getElementById("viewMoreProducts").innerHTML;
		if ( text == "更多产品..."){
			document.getElementById("viewMoreProducts").innerHTML = "精简列表";
		}else{
			document.getElementById("viewMoreProducts").innerHTML = "更多产品...";
		}
	
		$(".content-hide").each(function(){
			if ($(this).data('flag')=="hide"){
				$(this).show();
				$(this).data("flag", "show");
			}else{
				$(this).hide();
				$(this).data("flag", "hide");
			}
		});
	});

});
</script>
