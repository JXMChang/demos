<script>
    window.MAR = window.MAR || {};
    var siteIDQueryString = MAR.util.Page.getQueryString('siteId');
    var siteId = siteIDQueryString || PS.util.jspVars.get('IG_MAR.site');

    window.analytics = window.analytics || {};
    window.analytics.insight = {
        server: (window.location.host === "www.ig.com") ? "live" : "test",
        language: PS.util.Page.locale(),
        site: (siteId === "igcomuk") ? "igi" : siteId,
        pagename: "ft100",
        section: "market"
    };

    

    
    var miniPrices = new MiniPrices({
        epic: 'IX.D.FTSE.CFD.IP',
        modifiers: '',
        delayTime: '0',
        cptElement: document.getElementById('cpt'),
        cpcElement: document.getElementById('cpc'),
        delayElement: document.getElementById('delay')
    });
    miniPrices.init().startStreaming();
    

    

    var productDetails = new ProductDetails();
    productDetails.init();

    

    var longPercent = PS.util.jspVars.get('IG_MAR.longPercent');
    if (longPercent !== undefined) {
        var donut = new Donut(
            document.getElementById('donut'),
            longPercent,
            {
                segmentWidth: 20,
                gapWidth: 3,
                colors: {sell: '#da291c', buy: '#1484bc'},
                backgroundColor: '#ffffff'
            }
        );
    }

    

    
    

    function getChartOriginUrl() {
        var urlParts = document.location.hostname.split('.');
        var currentEnvironment = urlParts[0];

        switch (currentEnvironment) {
            case "net":
            case "net-local":
                return "https://net-api.ig.com";
            case "web":
                return "https://web-api.ig.com";
            case "demo":
                return "https://demo-api.ig.com";
            // default to prod
            default:
                return "https://api.ig.com";
        }
    }

    var chartEnvironment = {
        origin: getChartOriginUrl()
    };

    function getChartTimezoneLink(key) {
        var timezoneLink = {
            'africa.json': 'https://a.c-dn.net/b/2fZmjT.json#africa.json',
            'all.json': 'https://a.c-dn.net/b/0WYNfx.json#all.json',
            'america.json': 'https://a.c-dn.net/b/3MCVb4.json#america.json',
            'asia.json': 'https://a.c-dn.net/b/2KIaxp.json#asia.json',
            'australia.json': 'https://a.c-dn.net/b/14yrWJ.json#australia.json',
            'etc.json': 'https://a.c-dn.net/b/4sMWVx.json#etc.json',
            'europe.json': 'https://a.c-dn.net/b/3Svbli.json#europe.json',
            'pacific.json': 'https://a.c-dn.net/b/2GKtLn.json#pacific.json'
        };

        return timezoneLink[key] || 'https://a.c-dn.net/b/0WYNfx.json#all.json';
    }

    var chart = new Chart({
        epic: 'IX.D.FTSE.CFD.IP',
        container: document.getElementById('containerChart'),
        locale: PS.util.Page.locale(),
        siteid: siteId,
        environment: chartEnvironment,
        loadingMessage: 'Loading chart...',
        user: 'anonymous',
        token: '5f753df53f9628a5448865871427992e06d45bc1779a0d3417b25a6cadbff64b01111'
    });
    chart.init();

    jq(window).resize(function () {
        chart.resizeMA();
    });
    
    
    

    

    if (MAR.cache && MAR.cache.donutsList) {
        // Define needed donut settings.
        var donutSettings = {
            segmentWidth: 20,
            gapWidth: 3,
            colors: {sell: '#da291c', buy: '#1484bc'},
            backgroundColor: '#ffffff'
        };
        // Get the canvas elements for drawing the additional donuts.
        var canvases = jq('canvas').filter(function (index) {
            return this.id !== 'donut';
        });
        // Draw the additional donuts for the rest of market objects.
        for (var i = 0, len = MAR.cache.donutsList.length; i < len; i++) {
            if (canvases[i]) {
                new Donut(canvases[i], MAR.cache.donutsList[i]['longPercent'], donutSettings, 'http://is5.prod1.iggroup.local/ws-ma-fragment');
            }
        }
    }

    

    new ClientRedirect('FT100', 'FT100').setup();


    
    function fetchMISOrigins(data) {
        if (typeof data === "string") {
            try {
                data = JSON.parse(data);
            }
            catch (ex) {
                window.dlog('MIS: Error parsing data from server', ex);
            }

        }
        if (data && data.files && data.files['tracking-sdk-javascript.min.js']) {
            requirejs([data.files['tracking-sdk-javascript.min.js']], function (MISTracker) {
                window.IG = window.IG || {};
                window.IG.Tracker = new MISTracker();

                if (window.PS.util.Cookie && window.PS.util.Cookie.get && window.PS.util.Cookie.get('client_id')) {
                    window.IG.Tracker.initTracker('IG.BROWSER.WEBSITE', {trackingId: window.PS.util.Cookie.get('client_id')});
                } else {
                    window.IG.Tracker.initTracker('IG.BROWSER.WEBSITE');
                }

                window.IG.Tracker.trackEvent({
                    schema: 'igs:com.ig.tracking.sdk/market/interest/jsonschema/1.1.63',
                    epicId: 'IX.D.FTSE.CFD.IP',
                    marketId: 'FT100',
                    pageType: 'market'
                });

                window.IG.Tracker.trackEvent({
                    schema: 'igs:com.ig.tracking.sdk/register/recognition/jsonschema/1.1.63'
                });
            });

        } else {
            window.dlog('MIS: No returned library info');
        }
    }

    MAR.util.getAjax('https://www.ig.com/static-content-bootstrap/bootstrap/asset/trackingSdk',
        fetchMISOrigins);

</script>









    
        
        
