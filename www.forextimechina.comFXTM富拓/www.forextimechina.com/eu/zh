screen_width = document.documentElement.clientWidth;
// console.log('screen_width is ',screen_width);
fixFooterLinks(screen_width);
};
function fixFooterLinks(screen_width){
var footerMenu = document.getElementsByClassName("footer-nav");
var toggleSet = 'dropdown';
if(screen_width <= 767) {
toggleSet = '';
}
for( var item in footerMenu[0].children ){
if( item >=0 ){
// console.log('footerMenu[0].children[item].children["0"].children["0"].children["0"] is ',footerMenu[0].children[item].children[0].children[0].dataset.toggle);
footerMenu[0].children[item].children[0].children[0].dataset.toggle = toggleSet;
}
}
}
fixFooterLinks(screen_width);
});
</script>
